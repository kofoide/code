  <DataSets>
    <DataSet Name="SalesByDay">
      <Query>
        <DataSourceName>AdventureWorksDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        DimDate.DayNumberOfWeek, DimDate.EnglishMonthName, DimDate.MonthNumberOfYear, DimDate.CalendarYear, DimDate.FullDateAlternateKey, 
                         SUM(FactInternetSales.SalesAmount) AS TotalSales, SUM(FactInternetSales.TotalProductCost) AS TotalCost, DimDate.DayNumberOfMonth, 
                         DimDate.EnglishDayNameOfWeek
FROM            DimDate LEFT OUTER JOIN
                         FactInternetSales ON DimDate.DateKey = FactInternetSales.OrderDateKey
WHERE        (DimDate.CalendarYear = @Year) AND (DimDate.MonthNumberOfYear = @Month)
GROUP BY DimDate.DayNumberOfWeek, DimDate.EnglishMonthName, DimDate.MonthNumberOfYear, DimDate.CalendarYear, DimDate.FullDateAlternateKey, 
                         DimDate.DayNumberOfMonth, DimDate.EnglishDayNameOfWeek
ORDER BY DimDate.FullDateAlternateKey</CommandText>
      </Query>
      <Fields>
        <Field Name="DayNumberOfWeek">
          <DataField>DayNumberOfWeek</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="EnglishMonthName">
          <DataField>EnglishMonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthNumberOfYear">
          <DataField>MonthNumberOfYear</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="FullDateAlternateKey">
          <DataField>FullDateAlternateKey</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalCost">
          <DataField>TotalCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DayNumberOfMonth">
          <DataField>DayNumberOfMonth</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="EnglishDayNameOfWeek">
          <DataField>EnglishDayNameOfWeek</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WeekNumber">
          <Value>=DatePart(DateInterval.WeekOfYear, CDate(Fields!FullDateAlternateKey.Value))</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Years">
      <Query>
        <DataSourceName>AdventureWorksDW</DataSourceName>
        <CommandText>SELECT        CalendarYear
FROM            DimDate
GROUP BY CalendarYear
ORDER BY CalendarYear</CommandText>
      </Query>
      <Fields>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Months">
      <Query>
        <DataSourceName>AdventureWorksDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        MonthNumberOfYear, EnglishMonthName
FROM            DimDate
WHERE        (CalendarYear = @Year)
GROUP BY MonthNumberOfYear, EnglishMonthName
ORDER BY MonthNumberOfYear</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthNumberOfYear">
          <DataField>MonthNumberOfYear</DataField>
